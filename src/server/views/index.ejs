            <li><a data-value="de" class="padded-list">DE</a></li>
        </ul>
    </div-->

    <!-- PAGES -->
    <sleeper-alarms data-page="true"></sleeper-alarms>
    <sleeper-alarms-add data-page="true"></sleeper-alarms-add>
    <sleeper-alarms-add-time data-page="true"></sleeper-alarms-add-time>
    <sleeper-alarms-add-personality data-page="true"></sleeper-alarms-add-personality>
    <sleeper-alarms-add-questions data-page="true"></sleeper-alarms-add-questions>
    <sleeper-alarm-summary data-page="true"></sleeper-alarm-summary>
    <sleeper-welcome data-page="true"></sleeper-welcome>
    <sign-up-contact data-page="true"></sign-up-contact>
    <sign-up-verify data-page="true"></sign-up-verify>
