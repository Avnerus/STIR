    <rouser-alarm-record data-page="true"></rouser-alarm-record>
    <rouser-alarm-mix data-page="true"></rouser-alarm-mix>
    <rouser-alarm-thankyou data-page="true"></rouser-alarm-thankyou>
    <rouser-alarm-mturk data-page="true"></rouser-alarm-mturk>
    <rouser-welcome data-page="true"></rouser-welcome>
    <admin-login data-page="true"></admin-login>
    <admin-dashboard data-page="true"></admin-dashboard>

    <script src="/bundle.js"></script>

    <%- typeof nfbFooter != 'undefined' ? nfbFooter.join('\n') : '' %>

