  <script>
    window.IS_CLIENT = true;
    window.IS_SERVER = false;

    var initialData = '<%- initialData %>'
  </script>
  </head>
  <body <%- typeof nfbTopnav != 'underfined' ? 'class="' + nfbTopnav + '"' : '' %> >
    <%- typeof nfbHeader != 'undefined' ? nfbHeader.join('\n') : '' %>
    <%- body %>

    <!--Home suggest panel-->
    <div id="home-suggest" class="panel">
            <div class="content">
                <div class="padded-full">
                    <span id="home-suggest-message">Add this page to your home screen!</span>
                    <a href="" data-panel-close="true"><i class="material-icons">clear</i></a>
                </div>
            </div>
    </div>

    <!-- Intro Panels -->
    <div id="intro-panel" class="intro-panel panel-full">
        <div class="content">
            <video playsinline="">
                <source src="https://rawgit.com/bower-media-samples/big-buck-bunny-480p-30s/master/video.mp4">
            </video>

            <div class="overlay">
                <h1 class="video-title"> Welcome to STIR</h1>
                <a class="play-button" href=""><i class="material-icons">play_arrow</i></a>
                <a class="skip-link" href="">Skip</a>
            </div>
        </div>
    </div>
    <div id="rouser-intro-panel" class="intro-panel panel-full">
        <div class="content">
            <video playsinline="">
                <source src="https://rawgit.com/bower-media-samples/big-buck-bunny-480p-30s/master/video.mp4">
            </video>

            <div class="overlay">
                <h1 class="video-title"> Welcome</h1>
                <a class="play-button" href=""><i class="material-icons">play_arrow</i></a>
                <a class="skip-link" href="">Skip</a>
            </div>
        </div>
    </div>

    <!--Language Popover-->
